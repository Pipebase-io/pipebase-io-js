{
  "name": "relogs-io-js",
  "version": "0.1.0",
  "description": "NPM package for Relogs.io",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
