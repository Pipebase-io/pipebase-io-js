  "repository": {
    "type": "git",
    "url": "git+https://github.com/Relogs-io/relogs-io-js.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Relogs-io/relogs-io-js/issues"
  },
  "homepage": "https://github.com/Relogs-io/relogs-io-js#readme",
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@types/lodash": "^4.14.178",
    "@types/node": "^13.13.0",
    "@typescript-eslint/parser": "^2.28.0",
    "dotenv": "^14.3.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-jest": "^23.8.2",
    "jest": "^25.3.0",
    "prettier": "^2.0.4",
    "ts-jest": "^25.4.0",
    "typescript": "^3.8.3"
  },
  "files": [
    "dist/**/*"
  ],
